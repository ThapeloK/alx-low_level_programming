#include <stdlib.h>
#include <time.h>
#include <stdio.h>

/**
 *main - Print digit from 0 to 9 separate by coma and space
 *digit: represent each digit
 *
 * Return: 0 at the end
 */
int main(void)
{
	int num;

	for (num = '0'; num <= '9'; num++)
	{
		putchar(num);
		if (num != '9')
		{
			putchar(',');
			putchar(' ');
		}
	}
	putchar('\n');
	return (0);
}